{
  "name": "@modusoperandi/licit-changecase",
  "version": "1.0.0",
  "type": "module",
  "subversion": "1",
  "description": "changecase plugin built with ProseMirror",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "rm -rf dist/ coverage/ bin/ && rm -f modusoperandi-licit-changecase-*.*.*.tgz",
    "lint:ts": "eslint src --ext .ts,.tsx --fix",
    "lint": "npm run lint:ts",
    "ci:build": "tsc --build && copyfiles --up 1 \"src/**/*.css\" dist  && copyfiles package.json dist",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test": "jest --coverage",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
    "ci:bom": "cyclonedx-bom -o dist/bom.xml"
  },
  "peerDependencies": {
    "@modusoperandi/licit-ui-commands": "^1.0.0"
  },
  "devDependencies": {
    "@cyclonedx/bom": "^4.1.0",
    "@testing-library/dom": "^10.3.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^5.40.1",
    "copyfiles": "^2.4.1",
    "enzyme": "^3.11.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "husky": "^9.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "jest-json": "^2.0.0",
    "jest-junit": "^16.0.0",
    "jest-prosemirror": "^2.1.5",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3"
  },
  "overrides": {
    "semver": "^7.5.2"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "!(*test|*.setup).ts": [
    "eslint --fix"
  ],
  "*.json": [
    "prettier --write"
  ]
}